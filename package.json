{
  "name": "wmail",
  "version": "1.3.1",
  "prerelease": false,
  "description": "The missing desktop client for Gmail & Google Inbox",
  "scripts": {
    "prestart": "webpack",
    "start": "electron bin/app/index.js",
    "test": "standard",
    "package:darwin": "node packager.js darwin",
    "package:linux": "node packager.js linux",
    "package:win32": "node packager.js win32",
    "package": "node packager.js darwin && node packager.js linux && node packager.js win32",
    "postinstall": "electron-rebuild",
    "install-all": "echo ':>wmail'; npm install; cd src/app; echo ':>wmail-app'; npm install; cd ../../src/scenes/mailboxes; echo ':>wmail-windows-mailboxes'; npm install",
    "outdated-all": "echo ':>wmail'; npm outdated; cd src/app; echo ':>wmail-app'; npm outdated; cd ../../src/scenes/mailboxes; echo ':>wmail-windows-mailboxes'; npm outdated"
  },
  "keywords": [],
  "author": "Thomas Beverley",
  "license": "BSD-2-Clause",
  "repository": "https://github.com/Thomas101/wmail",
  "main": "bin/app/index.js",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^2.1.5",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "1.0.2",
    "electron-rebuild": "^1.1.3",
    "electron-winstaller": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.0",
    "less-loader": "^2.2.3",
    "nlf": "^1.4.0",
    "standard": "^7.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "devDependencies": {
    "standard": "^7.0.1"
  }
}
